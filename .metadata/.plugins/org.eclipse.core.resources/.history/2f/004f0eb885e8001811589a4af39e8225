package IntervalInterviewQuestion.interval;

import static org.junit.Assert.assertEquals;

import java.util.ArrayList;
import java.util.List;

import org.junit.Test;

public class IntevalTest {
	
	@Test
	public void test_one_interval_getSize() {
		
		//arrange
		Integer[] a= {7,10};
		List<Integer[]> intevalList = new ArrayList<Integer[]>();
		intevalList.add(a);
		IntervalMerger mergeInterval = new IntervalMerger(List<Integer[]> intervalList);

		//act
		mergeInterval.addInterval(a);
		int[] result = mergeInterval.getResult();
		
		//assert
		assertEquals(1, result);
		
		
		
		
	}

}
