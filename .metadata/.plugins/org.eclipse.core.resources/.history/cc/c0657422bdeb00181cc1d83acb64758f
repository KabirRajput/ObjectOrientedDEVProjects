package com.fdm.threading;

public class Runner {

	public static void main(String[] args) {

		String name = new String("name");

		ThreadExample threadExample = new ThreadExample(name);
		threadExample.start();
		
		RunnableExample runnableExample = new RunnableExample(name);
		Thread runnableExample = new Thread(runnableExample);
		runnableExample.start();
	}
}
