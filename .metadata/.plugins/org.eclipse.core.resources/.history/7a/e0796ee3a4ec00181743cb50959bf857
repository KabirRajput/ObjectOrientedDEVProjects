package com.java.ood.elevatorproject.model;

import java.util.List;

public class ElevatorLogic {
	private List<Elevator> elevatorList;

	public ElevatorLogic(List<Elevator> elevatorList) {
		this.elevatorList = elevatorList;
	}

	public void assignFloor(int elevatorIndex, int destinationFloor) {
		// logic to decide which elevator to assign
		elevatorList.get(elevatorIndex).floorList.add(destinationFloor);
		
	}

}
