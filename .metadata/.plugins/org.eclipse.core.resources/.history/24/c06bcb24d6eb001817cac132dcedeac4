package OOD3_Elevator_Kabir.elevatorProject;

public class Elevator {

	private int floor;
	Direction dir;

	public int go(int i) {
		return 0;
	}

	public void moveUp{
		dir = Direction.Up;
	}

	public void moveDown{
		dir = Direction.Down
	}

	public boolean isMoving{
		returns dir.equals(Direction.STATIONARY);
	}

}
